  <Label layout="20.0,10.0,210.0,30.0" text="BACNet Parameters" font="20.0pt Tahoma" foreground="white" background="blue"/>

  <ImageButton layout="45.0,60.0,100.0,135.0" text="MAIN" font="bold 16.0pt Tahoma">
    <ValueBinding hyperlink="station:|slot:/"/>
    <WsAnnotation name="wsAnnotation" value="10,10,8"/>
  </ImageButton>

  <ImageButton layout="45.0,240.0,100.0,135.0" text="Fine   Tuning" font="bold 16.0pt Tahoma" wordWrapEnabled="true">
    <ValueBinding hyperlink="station:|slot:/FineTuning"/>
    <WsAnnotation name="wsAnnotation" value="10,10,8"/>
  </ImageButton>

</CanvasPane>
